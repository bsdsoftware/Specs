{
  "name": "Google-Admob-SDK-BSD",
  "version": "7.0.0",
  "summary": "Google AdMob Ads SDK.",
  "description": "The Google AdMob Ads SDK allows developers to easily incorporate mobile-friendly text and image banners as well as rich, full-screen web apps known as interstitials.",
  "homepage": "https://developers.google.com/mobile-ads-sdk/docs/",
  "license": {
    "type": "Copyright",
    "text": "Copyright 2009 - 2014 Google, Inc. All rights reserved.\n"
  },
  "authors": "Google Inc.",
  "source": {
    "git": "https://github.com/bsdsoftware/AdMob.git",
    "tag": "7.0.0"
  },
  "platforms": {
    "ios": "6.0"
  },
  "source_files": [
    "GoogleMobileAds.framework/Versions/A/Headers/*.h"
  ],
  "preserve_paths": "GoogleMobileAds.framework/*",
  "frameworks": [
  	"GoogleMobileAds",
    "AVFoundation",
    "AudioToolbox",
    "CoreTelephony",
    "MessageUI",
    "SystemConfiguration",
    "CoreGraphics",
    "AdSupport",
    "StoreKit",
    "EventKit",
    "EventKitUI"
  ],
  "xcconfig": {
    "LIBRARY_SEARCH_PATHS": "\"$(PODS_ROOT)/Google-Admob-SDK-BSD/\""
  },
  "requires_arc": false
}
